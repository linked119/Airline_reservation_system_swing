        <AuxValue name="JavaCodeGenerator_CreateCodePre" type="java.lang.String" value="// getting data&#xa;try {&#xa;            Connection conn = DriverManager.getConnection(&quot;jdbc:mysql://localhost/airline&quot;, &quot;root&quot;, &quot;&quot;);&#xa;            Statement stmt = conn.createStatement();&#xa;            ResultSet rs = stmt.executeQuery(&quot;SELECT COUNT(*) AS total FROM passengers&quot;);&#xa;            if(rs.next()){&#xa;                jLabel4.setText(rs.getString(&quot;total&quot;));&#xa;            }&#xa;        &#xa;            rs = stmt.executeQuery(&quot;SELECT count(*) as total_cancel_flights FROM passengers WHERE status = &apos;cancel&apos;&quot;);&#xa;            if(rs.next()){&#xa;                jLabel5.setText(rs.getString(&quot;total_cancel_flights&quot;));&#xa;            }&#xa;        &#xa;            rs = stmt.executeQuery(&quot;SELECT if(SUM(cost) IS null ,0,SUM(cost)) AS total_cost FROM passengers WHERE status = &apos;active&apos;&quot;);&#xa;            if(rs.next()){&#xa;                System.out.println(rs.getString(&quot;total_cost&quot;));&#xa;                jLabel6.setText(&quot;$&quot;+rs.getString(&quot;total_cost&quot;));&#xa;            }&#xa;&#xa;        } catch (SQLException e) {&#xa;            e.printStackTrace();&#xa;        }&#xa;// getting data"/>
